//multiplication
func testOperators_1() -> Unit!1 {
    let x: Int!3 = 1
    let y = 2 * x
    assertTypeEqual(x, Int!1)
    assertTypeEqual(y, Int!1)

    let z: Float!inf = 10.0 * 20.0
    assertTypeEqual(z, Float!inf)

    //multiplication with exponential types is possible even with two variables
    let a: Int!inf = 4
    let b: Int!inf = 6
    let c = a * b

    let d: Float!10 = 4.5
    let e = d * 0.5
    assertTypeEqual(d, Float!9)

    let f = -3.4 * d
    assertTypeEqual(d, Float!5.6)

    return ()
}
